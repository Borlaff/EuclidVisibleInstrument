[TEST]
#size of the output array
xsize = 2048
ysize = 2066

#dark noise in electrons per second
dark = 0.179

#readout noise in electrons [default=4.5]
readout = 4.5

#bias level to add in electrons [default=1000]
bias = 1000.0

#background in photoelectrons [median=97]
cosmic_bkgd = 97.0

#electron/ADU conversion factor
e_ADU = 3.5

#zero point (electrons / seconds for magnitude zero)
magzero = 1.7059e10

#exposure to simulate, exposure time and position on the sky (in degrees)
exposures = 1
exptime = 565.0
RA = 145.95
DEC = -38.16

#input sources
sourcelist = source_test.dat

#name of the output file
output = test.fits

#Control flags (can be yes/no, true/false, 1/0)
#add noise?
noise = yes

#apply cosmetic defects?
cosmetics = no

#apply charge injection?
chargeInjection = no

#apply radiation damage model?
radiationDamage = no

#add cosmic rays?
cosmicRays = no

#apply flatfield, either multiplicative (M) or additive (A)?
flatfieldM = no
flatfieldA = no


